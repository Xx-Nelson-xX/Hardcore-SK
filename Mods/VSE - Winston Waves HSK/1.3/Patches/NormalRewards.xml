						  <li>Leathers</li>
						  <li>BTextiles</li>
						</thingCategories>
						<count>400</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
