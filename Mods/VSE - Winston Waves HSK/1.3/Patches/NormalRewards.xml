<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalResourceDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 250 wood, 250 steel, 250 stone blocks, 10 components</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalResourceDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>WoodLog</thing>
						<count>250</count>
					  </li>
					  <li>
						<thing>Plasteel</thing>
						<count>250</count>
					  </li>
					  <li>
						<thing>ComponentIndustrial</thing>
						<count>10</count>
					  </li>
					</items>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalResourceDrop"]</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>StoneBlocks</li>
						</thingCategories>
						<count>250</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalComponentDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 35 medieval components, 20 industrial components, 5 advanced components</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalComponentDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>ComponentMedieval</thing>
						<count>35</count>
					  </li>
					  <li>
						<thing>ComponentIndustrial</thing>
						<count>20</count>
					  </li>
					  <li>
						<thing>ComponentAdvanced</thing>
						<count>5</count>
					  </li>
					</items>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalFoodAndMedicineDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 30 simple meals and 30 medicine</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalFoodAndMedicineDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>MealSimple</thing>
						<count>15</count>
					  </li>
					  <li>
						<thing>RoastedMeat</thing>
						<count>15</count>
					  </li>
					  <li>
						<thing>HerbMedicine</thing>
						<count>30</count>
					  </li>
					</items>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalFuelDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 250 firewood and 250 charcoal</description>
				</value>
