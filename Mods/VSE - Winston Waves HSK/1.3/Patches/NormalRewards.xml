						<quality MayRequire="rebelrabbit.qualitybionics">Normal</quality>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalWeaponsDrop"]/randomItems</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>SurvivalToolsNeolithic</li>
						  <li>CrossbarRanged</li>
						  <li>Pistols</li>
						  <li>Shotguns</li>
						  <li>SMGs</li>
						  <li>Rifles</li>
						</thingCategories>
						<excludeThingCategories>
						  <li MayRequire="CETeam.CombatExtended">Ammo</li>
						</excludeThingCategories>
						<quality>Normal</quality>
						<count>3</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
