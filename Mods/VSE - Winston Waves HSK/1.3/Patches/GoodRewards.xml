<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodResourceDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 500 wood, 500 steel, 500 stone blocks, 30 industrial components and 200 melchior alloy</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodResourceDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>WoodLog</thing>
						<count>500</count>
					  </li>
					  <li>
						<thing>Plasteel</thing>
						<count>500</count>
					  </li>
					  <li>
						<thing>CupronickelAlloy</thing>
						<count>200</count>
					  </li>
					  <li>
						<thing>ComponentIndustrial</thing>
						<count>30</count>
					  </li>
					</items>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodResourceDrop"]</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>StoneBlocks</li>
						</thingCategories>
						<count>500</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodComponentDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 75 medieval components, 60 industrial components, 15 advanced components and 5 spacer components</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodComponentDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>ComponentMedieval</thing>
						<count>75</count>
					  </li>
					  <li>
						<thing>ComponentIndustrial</thing>
						<count>60</count>
					  </li>
					  <li>
						<thing>ComponentAdvanced</thing>
						<count>15</count>
					  </li>
					  <li>
						<thing>ComponentSpacer</thing>
						<count>5</count>
					  </li>
					</items>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodFuelDrop"]/description</xpath>
				<value>
					<description>Cargo pods drop 800 chemfuel, 800 coal, 500 firewood and 250 uranium</description>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodFuelDrop"]/items</xpath>
				<value>
					<items>
					  <li>
						<thing>Chemfuel</thing>
						<count>800</count>
					  </li>
					  <li>
						<thing>Coal</thing>
						<count>800</count>
					  </li>
					  <li>
						<thing>Kindling</thing>
						<count>500</count>
					  </li>
					  <li>
						<thing>Uranium</thing>
						<count>250</count>
					  </li>
					</items>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodTextileDrop"]/randomItems</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>Leathers</li>
						  <li>BTextiles</li>
						  <li>Wools</li>
						</thingCategories>
						<count>1000</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_NormalProstheticsDrop"]/randomItems</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>SimpleProstheses</li>
						</thingCategories>
						<count>5</count>
						<quality MayRequire="rebelrabbit.qualitybionics">Normal</quality>
					  </li>
					  <li>
						<thingCategories>
						  <li>BodyPartsBionic</li>
						</thingCategories>
						<count>5</count>
						<quality MayRequire="rebelrabbit.qualitybionics">Normal</quality>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodWeaponsDrop"]/randomItems</xpath>
				<value>
					<randomItems>
					  <li>
						<thingCategories>
						  <li>SurvivalToolsNeolithic</li>
						  <li>SurvivalToolsIndustrial</li>
						  <li>SpecialMelee</li>
						  <li>Pistols</li>
						  <li>Shotguns</li>
						  <li>SMGs</li>
						  <li>Rifles</li>
						  <li>SRifles</li>
						  <li>MachineGun</li>
						</thingCategories>
						<excludeThingCategories>
						  <li MayRequire="CETeam.CombatExtended">Ammo</li>
						</excludeThingCategories>
						<quality>Good</quality>
						<count>3</count>
					  </li>
					</randomItems>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/VSEWW.RewardDef[defName="VSEWW_GoodArmorDrop"]/randomItems/li[1]/thingCategories</xpath>
				<value>
				  <li>SetMedieval</li>
				  <li>LightUpperArmorCat</li>
				  <li>LightFullArmorCat</li>
				  <li>LightHelmetsCat</li>
				  <li>MediumUpperArmorCat</li>
				  <li>SetSyndicate</li>
				</value>
			</li>
		</operations>
